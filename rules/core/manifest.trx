§manifest|
    name:Manifest;
    description:Manifest file for the TRX language;
    extension:trx;
    version:1.2;
    author:Markus;
    license:MIT;
    scope:global;
    encoding:UTF-8;
    entrypoint:§boot;

    symbols: \\|, _, \\, !, ?, #, §, @, :, ;, ,, . ;
    identifier:[A-Za-z_][A-Za-z0-9_]*;
    whitespace: +;
    newline:,; ;
    newpage:;; ;
    escape:\\|;
    comment:comment,ignore;
_|